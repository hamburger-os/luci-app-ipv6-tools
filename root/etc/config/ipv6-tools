config ipv6-tools 'global'
    option enable '0'
    option debug_level '1'
    option check_interval '5'
    option idle_timeout '300'
    option startup_delay '10'

config openclash 'auto_control'
    option enable '1'
    option check_method 'domain'
    option domain_list ''
    option ip_list ''
    option start_command '/etc/init.d/openclash restart'
    option stop_command '/etc/init.d/openclash stop'
    option status_command '/etc/init.d/openclash status'

config traffic_monitor 'traffic'
    option enable '1'
    option interface 'all'
    option min_traffic_kb '10'
    option check_interval '10'

config logging 'log'
    option enable '1'
    option log_file '/var/log/ipv6-tools.log'
    option max_size '100'
    option log_level 'info'